{
	"dev": {
		"region": "europe-west1",
		"project": "monks-plexus",
		"serviceAccount": "plexus@monks-plexus.iam.gserviceaccount.com",
		"env": [
			"OPENAI_API_KEY",
			"AZURE_TENANT_ID",
			"AZURE_CLIENT_ID",
			"AZURE_CLIENT_SECRET"
		],
		"apiBaseUrl": "/api",
		"functions": [
			{
				"name": "api",
				"minInstances": 1,
				"cpus": 1,
				"memory": "1GiB",
				"entryPoint": "api",
				"timeout": 300
			}
		],
		"apiGateway": {
			"api": "plexus-api",
			"gateway": "plexus-gateway",
			"config": "plexus-config"
		}
	}
}
