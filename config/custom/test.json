{
	"profiling": true,
	"dataDumps": false,
	"debug": true,
	"llm": {
		"platform": "local"
	},
	"agents": {
		"hello": {
			"instructions": "gs://monks-plexustest/hello.txt",
			"context": [
				"names"
			]
		},
		"names": {
			"instructions": "gs://monks-plexustest/names.txt",
			"context": [
				"documents"
			]
		},
		"test": {
			"instructions": "gs://monks-plexustest/hello.txt",
			"context": [
				"gcs-export"
			]
		},
		"summary": {
			"instructions": "Please summarize the provided documents. If user input is listed under `input`, reply to that instead.",
			"context": [
				"documents",
				"input"
			]
		},
		"image": {
			"instructions": "Describe the provided image.",
			"context": [
				"image"
			]
		}
	},
	"catalog": {
		"name": {
			"type": "input",
			"field": "name",
			"required": true
		},
		"helloString": {
			"type": "output",
			"agent": "hello",
			"field": "helloString",
			"example": "the output string"
		},
		"documents": {
			"type": "data",
			"dataSource": "documents",
			"example": [
				"someFile.pdf",
				"someOtherFile.pdf"
			]
		},
		"names": {
			"type": "output",
			"agent": "names",
			"field": "names",
			"example": [
				"John",
				"Pete"
			]
		},
		"gcs-export": {
			"type": "data",
			"source": "gcs-export"
		},
		"output": {
			"type": "output",
			"agent": "test",
			"field": "output",
			"example": "the output string"
		},
		"summary": {
			"type": "output",
			"agent": "summary",
			"field": "summary",
			"example": "This is the summary."
		},
		"input": {
			"type": "input",
			"field": "input",
			"example": "Please explain your findings."
		},
		"image": {
			"type": "input",
			"field": "image",
			"example": "some_base64_string",
			"fileName": "image.png",
			"mimeType": "image/png"
		},
		"image-description": {
			"type": "output",
			"field": "description",
			"example": "The description of the image",
			"agent": "image"
		}
	},
	"data-sources": {
		"documents": {
			"namespace": "test",
			"uri": "https://drive.google.com/drive/folders/1o8kMT0Fra7KpZHkB9Ud7CkjbzlEaJtFG",
			"platform": "drive",
			"target": "file",
			"folder": true
		},
		"gcs-export": {
			"namespace": "test",
			"uri": "gs://monks-plexustest/gcs-export",
			"target": "file",
			"folder": true
		}
	},
	"output": [
		"image-description"
	]
}
